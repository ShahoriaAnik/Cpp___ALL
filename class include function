#include <iostream>
#include<string>

using namespace std;

class university{
 private:
    string v_name;
    int v_dept_number;
    int total_stu_per_dept;
  
    int total_teacher_per_dept;
  public:
    void value(string s,int a,int b,int c){
        v_name=s;
        v_dept_number=a;
        total_stu_per_dept=b;
        total_teacher_per_dept=c;
        
    }
    void Print(){
        cout<<v_name<<endl<<v_dept_number<<endl<<total_stu_per_dept<<endl<<total_teacher_per_dept<<endl;
    }
    
};

int main()
{
    university v1,v2,v3;
    v1.value("iubat",13,100,5);
    v2.value("diu",13,100,5);
    v3.value("ut",13,100,5);
    v1.Print();
    v2.Print();
   /* cout<<"enter university1 name: ";
